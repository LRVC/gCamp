<div class="container">
  <div class="page-header">
    <div class="pull-right">
      <%= link_to 'New Project', new_project_path, class: "btn btn-info" %>
    </div>
    <h3>Projects</h3>
  </div>


  <table class= "table">
    <thead>
      <tr>
        <th> Name </th>
      </tr>
    </thead>

    <% @projects.each do |project| %>

      <tr>
        <td>
            <%= project.name%>
        </td>

      </tr>
    <% end %>
  </table>
  </div>
</div>
